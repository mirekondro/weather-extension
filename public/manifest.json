{
  "manifest_version": 3,
  "name": "Weather Extension",
  "version": "1.0",
  "description": "Shows current weather",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/YW16.png",
      "32": "icons/YW32.png",
      "48": "icons/YW48.png",
      "128": "icons/YW128.png"
    }
  },
  "icons": {
      "16": "icons/YW16.png",
      "32": "icons/YW32.png",
      "48": "icons/YW48.png",
      "128": "icons/YW128.png"
  },
  "permissions": ["geolocation"]
}
